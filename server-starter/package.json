{
  "name": "server_name",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "monitor": "(npm run prestart)  && (nodemon  src/server.ts  dotenv_config_path=env/local.env dotenv_config_debug=true)",
    "build_docker_image_with_version": "(npm run  build) && (docker build -f Dockerfile  -t custom_image_name:$npm_package_version  .)",
    "build_docker": "(npm run  build) && (docker build -f Dockerfile  -t custom_image_name  .)",
    "start_docker_local_dev": "docker  run -p 3000:3000 --env-file=./env/docker.env  -it custom_image_name",
    "start_docker_local_com": "(npm run  build) && (docker-compose build --no-cache) && (docker images | awk '{print $1}' | awk 'NR==2')&& (docker-compose up)",
    "startProd": "node server.js",
    "tsrun": "ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "await-to-js": "^3.0.0",
    "axios": "^1.2.6",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "mongoose": "^6.8.1",
    "qs": "^6.11.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "^2.0.44",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.17",
    "@types/node-fetch": "^2.6.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  }
}
